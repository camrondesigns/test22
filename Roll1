    	$('#load_more').click(function(){    		var hidden_articles = $('.softicons-content.d-none:first');
			if(hidden_articles.length > 0){
				$(hidden_articles).removeClass('d-none');
				var hidden_articles = $('.softicons-content.d-none:first');
				$(hidden_articles).removeClass('d-none');
			}

			// count again
            var hidden_a = $('.softicons-content.d-none:first');
            if(hidden_a.length == 0){            	$(this).hide();
            }    	})
    </script>

