<script type="text/javascript">
            let papScript = document.createElement("script");
            papScript.setAttribute("src", "https://affiliates.a2hosting.com/scripts/9u7wajf7le4");
            papScript.setAttribute("id", 'pap_x2s6df8d');
            papScript.setAttribute("type", "application/javascript");
            papScript.setAttribute("onload", 'a2_pap_setup();');
            document.body.appendChild(papScript);

            function a2_pap_setup() {
                PostAffTracker.setAccountId('default1');
                PostAffTracker.disableTrackingMethod('F');
                try { PostAffTracker.track(); } catch (err) { }

                var new_pap_subid = $.urlParam('subid');

                var current_pap_subid = a2.cookie.get('PAPSubId');

                if (new_pap_subid !== false) {
                    if (current_pap_subid === null || current_pap_subid.length == 0) {
                        a2.cookie.set('PAPSubId', new_pap_subid, null, '.a2hosting.com');
                    }
                }
            }

            $.urlParam = function (name) {
                var results = new RegExp('[\?&]' + name + '=([^&#]*)')
                    .exec(window.location.search);

                return (results !== null) ? results[1] || 0 : false;
            };
        </script>
<script type="text/javascript" src="https://www.a2hosting.com/wp-content/themes/a2hosting/assets/a2layouts/9u7wajf7le4.js?v=1.2.2" id="pap_x2s6df8d" onload="a2_pap_setup();"></script>
<script type="text/javascript" src="https://www.a2hosting.com/wp-content/themes/a2hosting/assets/a2layouts/9u7warf7le4.js?v=1.2.2"></script>

