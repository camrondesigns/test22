<script>
		jQuery(document).ready(function () {

			jQuery("#a2-hellobar").prependTo("body");

			jQuery(".a2-hellobar-close").click(function(){
				jQuery("#a2-hellobar").hide();
			})

			var form = document.querySelector("#hb_form");
			form.addEventListener("submit", async function(event) {
				event.preventDefault();

				jQuery(".invalid-feedback").hide();
				var form_error = false;

				jQuery("#hb_form input.form-control").each(function(){
					if(jQuery(this).val() == ''){
						jQuery(".invalid-" + this.id).show();
						form_error = true;
					}
				});

				if(form_error == false){
					const scriptURL = "https://script.google.com/macros/s/AKfycbxPyUhasKfopCmGU_LktOHn_rhBCfwsTCtj8QPU3SLfxK68JBRiW5LDwrdug3w2QM9A/exec";
					const form = document.forms["hb_form"]
                    jQuery("#hb-submit-button").text("Please Wait...").attr("disabled", true);

					const response = await fetch(scriptURL, { method: "POST", body: new FormData(form)});

					if (response.ok) {
						const data = await response.json();
						if (data.result == "success") {
					      	jQuery(".hellobarformel, .hellobaractions, .hellobarquote").addClass("d-none");
					      	jQuery(".hellobarsuccessmsg, .hellobarcontinue, .hellobarsuccess").removeClass("d-none");
						} else {
							jQuery("#hb-submit-button").text("Submit").attr("disabled", false);
						}
					} else {
						jQuery("#hb-submit-button").text("Submit").attr("disabled", false);
					}
				} else {
					grecaptcha.execute();
				}


			});


		});
		function hellobar_gtm() {
		    dataLayer.push({'event':'Clicked','event_category':'Button','event_label': 'HelloBar'});
			return false;
		}
	</script>

